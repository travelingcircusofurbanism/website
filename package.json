{
  "name": "travelingcircusofurbanism",
  "version": "1.0.0",
  "description": "Urban narratives and practices, collected through travel",
  "author": "Mariko Sugita & Jasper Stephenson",
  "private": true,
  "engines": {
    "node": "v16.11.0"
  },
  "browserslist": "cover 99.5%",
  "scripts": {
    "v": "node -v",
    "analyze": "nuxt build -a",
    "cleanup": "rm -rf docs",
    "dev": "nuxt",
    "g": "npm run generate",
    "preview": "nuxt generate && serve ./docs",
    "generate": "nuxt generate && echo \"www.travelingcircusofurbanism.com\" > ./docs/CNAME",
    "golive": "npm run cleanup && git pull && git add . && git commit -m \"Auto-pushing to live\" && git push"
  },
  "dependencies": {
    "@nuxtjs/feed": "^2.0.0",
    "@nuxtjs/google-analytics": "^2.4.0",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "mapbox-gl": "^2.3.0",
    "markdown-it": "^12.0.6",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.27.0",
    "polylabel": "^1.1.0",
    "sass": "^1.46.0",
    "sass-loader": "^10.1.1",
    "sharp": "^0.28.3",
    "supercluster": "^7.1.3"
  },
  "devDependencies": {
    "@nuxtjs/sitemap": "^2.4.0",
    "vue-lazyload": "^1.3.3"
  }
}

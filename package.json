{
  "name": "travelingcircusofurbanism",
  "version": "1.0.0",
  "description": "Urban narratives and practices, collected through travel",
  "author": "Mariko Sugita & Jasper Stephenson",
  "private": true,
  "scripts": {
    "analyze": "nuxt build -a",
    "autopush": "git add . && git commit -m \"Auto-pushing files to live\" && git push",
    "dev": "nuxt",
    "generate": "nuxt generate && echo \"www.travelingcircusofurbanism.com\" > ./docs/CNAME && mv ./docs/404/index.html ./docs/404.html && rm -rf ./docs/404/",
    "golive": "git pull && npm run generate && npm run autopush"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@nuxtjs/feed": "^0.1.1",
    "@nuxtjs/google-analytics": "^2.0.2",
    "axios": "^0.18.0",
    "mapbox-gl": "^0.45.0",
    "markdown-it": "^8.4.2",
    "node-sass": "^4.12.0",
    "nuxt": "^2.2.0",
    "polylabel": "^1.0.2",
    "sass-loader": "^7.1.0",
    "sharp": "^0.23.0",
    "supercluster": "^4.1.1",
    "vue-loader": "^14.2.3"
  },
  "devDependencies": {
    "@nuxtjs/sitemap": "^1.2.0",
    "vue-lazyload": "^1.2.6"
  }
}
